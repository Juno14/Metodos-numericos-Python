from numpy import*
a = array ([[3, 7, 5], [9, 4, 1], [2, 8, 3]], float)
b = eye(3)
c = concatenate([a, b], axis = 1)
print (c)
c[0, 0:] = c[0, 0:] / c[0, 0]
c[1, 0:] = c[1, 0:] - c[1, 0] * c[0, 0:]
c[2, 0:] = c[2, 0:] - c[2, 0] * c[0, 0:]
c[1, 1:] = c[1, 1:] / c[1, 1]
c[0, 1:] = c[0, 1:] - c[0, 1] * c[1, 1:]
c[2, 1:] = c[2, 1:] - c[2, 1] * c[1, 1:]
c[2, 2:] = c[2, 2:] / c[2, 2]
c[0, 2:] = c[0, 2:] - c[0, 2] * c[2, 2:]
c[1, 2:] = c[1, 2:] - c[1, 2] * c[2, 2:]
print(c[0:, 3:])
